{
  "name": "@cfxdevkit/contracts",
  "version": "1.0.11",
  "description": "Generated ABI, bytecode, and deployed addresses for Conflux DevKit contracts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.16",
    "tsup": "^8.5.1",
    "typescript": "^5.7.3",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "viem": ">=2.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cfxdevkit/devkit.git"
  },
  "homepage": "https://github.com/cfxdevkit/devkit#readme",
  "bugs": {
    "url": "https://github.com/cfxdevkit/devkit/issues"
  }
}
