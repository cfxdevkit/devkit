{
  "name": "@cfxdevkit/executor",
  "version": "1.0.5",
  "description": "Conflux DevKit â€“ on-chain strategy execution engine (limit orders, DCA, and more)",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "type-check": "tsc --noEmit",
    "dev": "tsup --watch",
    "test": "vitest run --pass-with-no-tests",
    "test:run": "vitest --run",
    "test:coverage": "vitest --run --coverage --pass-with-no-tests",
    "clean": "rm -rf dist",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "format": "biome format src/",
    "format:fix": "biome format --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --write src/"
  },
  "dependencies": {
    "@cfxdevkit/core": "workspace:*",
    "@cfxdevkit/protocol": "workspace:*",
    "viem": ">=2.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "keywords": [
    "conflux",
    "executor",
    "automation",
    "limit-order",
    "dca",
    "keeper",
    "blockchain",
    "web3"
  ],
  "author": "Conflux DevKit Team",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.11.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cfxdevkit/devkit.git",
    "directory": "packages/executor"
  },
  "homepage": "https://github.com/cfxdevkit/devkit#readme",
  "bugs": {
    "url": "https://github.com/cfxdevkit/devkit/issues"
  }
}
