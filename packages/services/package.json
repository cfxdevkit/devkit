{
  "name": "@cfxdevkit/services",
  "version": "1.0.13",
  "description": "High-level services for Conflux SDK (Swap, Keystore, Encryption)",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.js",
      "require": "./dist/services/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage --pass-with-no-tests",
    "clean": "rm -rf dist",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "format": "biome format src/",
    "format:fix": "biome format --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --write src/"
  },
  "keywords": [
    "conflux",
    "blockchain",
    "evm",
    "sdk",
    "viem",
    "cive",
    "defi",
    "espace",
    "core-space",
    "swappi",
    "wagmi"
  ],
  "author": "cfxdevkit",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cfxdevkit/devkit.git"
  },
  "homepage": "https://github.com/cfxdevkit/devkit#readme",
  "bugs": {
    "url": "https://github.com/cfxdevkit/devkit/issues"
  },
  "dependencies": {
    "@cfxdevkit/core": "workspace:*",
    "viem": "^2.43.3",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.11.0"
}
