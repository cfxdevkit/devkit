{
  "name": "@cfxdevkit/protocol",
  "version": "0.1.0",
  "description": "Conflux DevKit â€“ on-chain contract artifacts: ABIs, bytecode, and deployed addresses",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "type-check": "tsc --noEmit",
    "dev": "tsup --watch",
    "test": "vitest run --pass-with-no-tests",
    "test:run": "vitest --run",
    "test:coverage": "vitest --run --coverage",
    "clean": "rm -rf dist",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "format": "biome format src/",
    "format:fix": "biome format --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --write src/"
  },
  "dependencies": {},
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "keywords": [
    "conflux",
    "protocol",
    "abi",
    "contracts",
    "automation",
    "blockchain",
    "web3"
  ],
  "author": "Conflux DevKit Team",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.11.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cfxdevkit/conflux-devkit.git",
    "directory": "packages/protocol"
  },
  "homepage": "https://github.com/cfxdevkit/conflux-devkit#readme",
  "bugs": {
    "url": "https://github.com/cfxdevkit/conflux-devkit/issues"
  }
}
